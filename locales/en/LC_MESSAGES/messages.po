#: bot/utils.py:96
msgid "❗Ushbu kategoriya allaqachon mavjud."
msgstr "❗This category already exists."

#: bot/utils.py:107
msgid "✅ Ushbu kategoriya muvaffaqqiyatli saqlandi ✅!"
msgstr "✅ This category has been successfully saved ✅!"

#: bot/utils.py:114
msgid "Kategoriya qo'shishga urinishda xatolik yuz berdi."
msgstr "An error occurred while attempting to add the category."

#: bot/utils.py:120
msgid "Kategoriya qo'shishda xatolik yuz berdi."
msgstr "An error occurred while adding the category."

#: bot/buttons/inline.py:26
msgid "💾 Saqlash"
msgstr "💾 Save"

#: bot/buttons/keyboard.py:6 bot/handlers/category_selection.py:19
msgid "Saytlar 🌐"
msgstr "Websites 🌐"

#: bot/buttons/keyboard.py:7 bot/handlers/category_selection.py:24
msgid "Telegram kanallar 📣"
msgstr "Telegram channels 📣"

#: bot/buttons/keyboard.py:8 bot/handlers/category_selection.py:31
msgid "Profil 👤"
msgstr "Profile 👤"

#: bot/buttons/keyboard.py:37 bot/handlers/profile.py:18
msgid "Kategoriya qo'shish 🏷"
msgstr "Add Category 🏷"

#: bot/buttons/keyboard.py:38 bot/handlers/profile.py:22
msgid "Sayt qo'shish 🌐"
msgstr "Add Website 🌐"

#: bot/buttons/keyboard.py:39 bot/handlers/profile.py:27
msgid "Telegram kanal qo'shish 📣"
msgstr "Add Telegram Channel 📣"

#: bot/buttons/keyboard.py:40 bot/handlers/profile.py:33
msgid "Orqaga  ⤵️"
msgstr "Back ⤵️"

#: bot/handlers/add_category.py:25
msgid "Kategoriya uchun emoji yuboring:"
msgstr "Send an emoji for the category:"

#: bot/handlers/add_category.py:32
msgid "Iltimos, emoji yuboring."
msgstr "Please send an emoji."

#: bot/handlers/add_category.py:39
msgid "Iltimos, haqiqiy emoji yuboring."
msgstr "Please send a valid emoji."

#: bot/handlers/add_category.py:43
msgid "Bu emoji kiritilishi mumkin emas. Iltimos, boshqa emoji kiriting."
msgstr "This emoji cannot be used. Please send another emoji."

#: bot/handlers/add_category.py:56 bot/handlers/add_channel.py:78
#: bot/handlers/add_site.py:61
msgid "Siz profil bo'limiga qaytdingiz. Iltimos, boshqa amalni tanlang."
msgstr "You have returned to the profile section. Please choose another action."

#: bot/handlers/add_channel.py:21
msgid "Bu kanal nomi nomaqbul so'zlarni o'z ichiga oladi. Iltimos, boshqa nom kiriting:"
msgstr "This channel name contains inappropriate words. Please enter a different name:"

#: bot/handlers/add_channel.py:26
msgid "Kanal nomi qabul qilindi ✅!"
msgstr "Channel name accepted ✅!"

#: bot/handlers/add_channel.py:27
msgid "Endi, kanal usernameni yuboring ‼️"
msgstr "Now, please send the channel username ‼️"

#: bot/handlers/add_channel.py:38
msgid ""
"Iltimos, username ni t.me/ yoki @ formatida emas, faqat username ni "
"kiriting, Misol uchun ⤵️\n"
"\n"
"<i>t.me/kunuzofficial ❌ @kunuzofficial ❌ --> kunuzofficial ✅</i>"
msgstr ""
"Please enter only the username without the t.me/ or @ format. For example ⤵️\n"
"\n"
"<i>t.me/kunuzofficial ❌ @kunuzofficial ❌ --> kunuzofficial ✅</i>"

#: bot/handlers/add_channel.py:60
msgid "Ushbu kanal allaqachon sizga bog'langan 😊"
msgstr "This channel is already linked to you 😊"

#: bot/handlers/add_channel.py:64 bot/handlers/add_channel.py:73
msgid "Ushbu kanal foydalanuvchiga bog'landi 📣"
msgstr "This channel has been linked to the user 📣"

#: bot/handlers/add_channel.py:69
msgid "Ushbu kanal yangi kanal sifatida saqlandi 😊✅"
msgstr "This channel has been saved as a new channel 😊✅"

#: bot/handlers/add_site.py:28
msgid ""
"Iltimos, URL ni http:// yoki https:// bilan boshlanadigan formatda "
"yuboring."
msgstr "Please send the URL starting with http:// or https:// format."

#: bot/handlers/add_site.py:43
msgid "Ushbu sayt allaqachon sizga bog'langan 😊"
msgstr "This website is already linked to you 😊"

#: bot/handlers/add_site.py:47 bot/handlers/add_site.py:56
msgid "Ushbu sayt foydalanuvchiga bog'landi 🌐"
msgstr "This website has been linked to the user 🌐"

#: bot/handlers/add_site.py:52
msgid "Ushbu sayt yangi sayt sifatida saqlandi 😊✅"
msgstr "This website has been saved as a new site 😊✅"

#: bot/handlers/category_selection.py:22
msgid ""
"🌐 Siz veb saytlarni tanladingiz!\n"
"Sayt linklarini kiriting. Har birini alohida kiriting.\n"
"\n"
"🔗 <i>Misol uchun:</i> \n"
"https://example.uz/"
msgstr ""
"🌐 You have selected websites!\n"
"Enter the website links. Provide each one separately.\n"
"\n"
"🔗 <i>For example:</i> \n"
"https://example.uz/"

#: bot/handlers/category_selection.py:29
msgid ""
"🔔 Siz Telegram kanallarini tanladingiz!\n"
"\n"
"Kategoriya tanlang. Bir nechta kategoriya tanlab, ularni saqlashingiz "
"mumkin!"
msgstr ""
"🔔 You have selected Telegram channels!\n"
"\n"
"Select categories. You can choose multiple categories and save them!"

#: bot/handlers/category_selection.py:34
msgid "Profil bo'limi tanlandi!"
msgstr "Profile section selected!"

#: bot/handlers/category_selection.py:36
msgid "❗️Iltimos, quyidagi tugmalardan birini tanlang."
msgstr "❗️Please choose one of the buttons below."

#: bot/handlers/category_selection.py:64
msgid "Iltimos, kamida bitta toifani tanlang."
msgstr "Please select at least one category."

#: bot/handlers/category_selection.py:80
msgid "Kategoriyalar muvaffaqiyatli saqlandi ✅"
msgstr "Categories successfully saved ✅"

#: bot/handlers/category_selection.py:82
msgid "Kategoriyalar saqlandi va endi sizga muntazam yangiliklar yuboriladi 😊"
msgstr "Categories saved, and now you will receive regular updates 😊"

#: bot/handlers/category_selection.py:99
msgid "✅ Barcha saytlar saqlandi va sizga muntazam yangiliklar yuboriladi!"
msgstr "✅ All websites have been saved, and you will receive regular updates!"

#: bot/handlers/category_selection.py:101
msgid "❗️Hech qanday sayt saqlanmadi."
msgstr "❗️No websites were saved."

#: bot/handlers/category_selection.py:104
msgid "Siz bosh sahifaga qaytdingiz. Boshqa amal bajarishni hohlaysizmi?"
msgstr "You have returned to the main page. Would you like to perform another action?"

#: bot/handlers/category_selection.py:118
msgid "✅ Sayt(lar) muvaffaqiyatli saqlandi va hisobingizga qo'shildi."
msgstr "✅ Website(s) successfully saved and added to your account."

#: bot/handlers/category_selection.py:122
msgid "❗️Bu sayt(lar) allaqachon saqlangan"
msgstr "❗️These website(s) have already been saved."

#: bot/handlers/category_selection.py:127
msgid ""
"❗️Sayt linki formati noto'g'ri \n"
"Iltimos, qayta yuboring."
msgstr ""
"❗️The website link format is incorrect \n"
"Please resend."

#: bot/handlers/category_selection.py:129
msgid "⏳ Yana saytlarni yuboring yoki tugatish uchun /done ni bosing."
msgstr "⏳ Send more websites or press /done to finish."

#: bot/handlers/profile.py:19
msgid "Iltimos, kategoriya nomini yuboring: "
msgstr "Please send the category name:"

#: bot/handlers/profile.py:23
msgid ""
"🌐 Iltimos, sayt linklarini quyidagi formatda yuboring:\n"
"\n"
"https://example.com/"
msgstr ""
"🌐 Please send the website links in the following format:\n"
"\n"
"https://example.com/"

#: bot/handlers/profile.py:29
msgid "📣 Iltimos, kanal nomini yuboring:"
msgstr "📣 Please send the channel name:"

#: bot/handlers/profile.py:35
msgid "Siz orqaga qaytdingiz ⤵️"
msgstr "You have returned ⤵️"

#: bot/handlers/profile.py:39
msgid "Iltimos, tugmalardan birini tanlang‼️"
msgstr "Please choose one of the buttons‼️"

#: bot/handlers/start.py:45
msgid ""
"Til muvaffaqiyatli o'rnatildi!\n"
"\n"
"Iltimos tugmalardan birini tanlang⤵️"
msgstr ""
"Language successfully set!\n"
"\n"
"Please choose one of the buttons⤵️"
